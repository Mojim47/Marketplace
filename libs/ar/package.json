{
  "name": "@nextgen/ar",
  "version": "1.0.0",
  "description": "Augmented Reality components for NextGen Marketplace",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/model-viewer": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.3",
    "vitest": "^1.0.0",
    "fast-check": "^3.14.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "keywords": [
    "ar",
    "augmented-reality",
    "webxr",
    "model-viewer"
  ]
}
