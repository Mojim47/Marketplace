{
  "name": "@nextgen/ai",
  "version": "1.0.0",
  "description": "On-device AI utilities and prediction strategies for NextGen Marketplace",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@huggingface/tokenizers": "^0.1.1",
    "onnxruntime-node": "^1.20.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "vitest": "^1.0.0",
    "fast-check": "^3.14.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "keywords": ["ai", "prediction", "time-series", "on-device"]
}
